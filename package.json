{
  "name": "postcss-resolve-urls",
  "version": "0.0.7",
  "description": "A PostCSS plugin for resolving relative URLs",
  "keywords": [
    "postcss",
    "postcss-plugin",
    "relative url",
    "resolve url"
  ],
  "main": "./dist/index",
  "types": "./dist/index.d.ts",
  "browser": "./dist/index.js",
  "module": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "ts-node .build/esbuild.ts && tsc --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "npm run build"
  },
  "author": "bognarlaszlo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bognarlaszlo/postcss-resolve-urls.git"
  },
  "license": "MIT",
  "peerDependencies": {
    "postcss": "^8.3.11"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "esbuild": "^0.13.13",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
